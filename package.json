{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:slug",
    "test:slug": "npm run build && node dist/scripts/test-slug-utils.js",
    "test:redirect": "npm run build && node dist/scripts/test-canonical-redirect.js",
    "backfill:slugs": "node scripts/backfill-job-slugs.js",
    "dev": "cross-env TS_NODE_TRANSPILE_ONLY=1 TS_NODE_IGNORE_DIAGNOSTICS=2532,7006,7053,2554 tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "setup:db": "node scripts/setup-database.js",
    "migrate:mysql": "node scripts/migrate-to-mysql.js",
    "migrate:profile": "node src/scripts/add-profile-fields.js",
    "migrate:cv": "npx ts-node src/scripts/create-cv-files-table.ts",
    "migrate:resume": "node src/scripts/create-resumes-table.js",
    "migrate:enhance-jobs": "node src/scripts/enhance-jobs-table.js",
    "migrate:apply-url": "node src/scripts/add-apply-url-column.js",
    "migrate:aijobs": "npx ts-node src/scripts/create-aijobs-table.ts",
    "check:schema": "node test-db-schema.js",
    "test:profile": "node test-profile-update.js",
    "fix:db": "node src/scripts/fix-database-constraints.js",
    "sync:db": "node src/scripts/safe-database-sync.js",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down",
    "logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "backup:db": "node scripts/backup-database.js",
    "restore:db": "node scripts/restore-database.js"
  },
  "keywords": [],
